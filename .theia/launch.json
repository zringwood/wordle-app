{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    "version": "0.2.0",
    "configurations": [
      {
          "name": "Cloud Run: Run/Debug Locally",
          "type": "cloudcode.cloudrun",
          "request": "launch",
          "build": {
              "docker": {
                  "path": "Dockerfile"
              }
          },
          "image": "WordleSolverApp",
          "service": {
              "name": "wordlesolverapp",
              "containerPort": 8080,
              "resources": {
                  "limits": {
                      "memory": "256Mi"
                  }
              }
          },
          "target": {
              "minikube": {}
          },
          "watch": true
      }
  ]
  }
  
